<div class="row">
  <div class="col-md-6">
    <h3>Add Collaborator</h3>
    <% @users.each do |user| %>
      <%= button_to "Add", add_collaborator_wiki_path(user_id: user.id), method: :put %><%= user.email %><br />
    <% end %>
  </div>
  <div class="col-md-6">
    <h3>Current Collaborators</h3>
    <% @collaborators.each do |user| %>
      <%= button_to "Remove", remove_collaborator_wiki_path(user_id: user.id), method: :delete %><%= user.email %><br />
    <% end %>
  </div>
</div>
